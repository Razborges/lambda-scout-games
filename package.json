{
  "name": "lambda-scout-games",
  "version": "1.0.0",
  "description": "Serverless API for scout games",
  "main": "index.js",
  "scripts": {
    "start": "nodemon index.js",
    "update": "claudia update",
    "generate": "claudia generate-serverless-express-proxy --express-module app",
    "create": "claudia create --handler lambda.handler --deploy-proxy-api --region us-east-1 --policies policy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Razborges/lambda-scout-games.git"
  },
  "keywords": [
    "lambda",
    "express",
    "node",
    "claudia"
  ],
  "author": "RazBorges",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Razborges/lambda-scout-games/issues"
  },
  "homepage": "https://github.com/Razborges/lambda-scout-games#readme",
  "dependencies": {
    "aws-sdk": "^2.578.0",
    "aws-serverless-express": "^3.3.6",
    "express": "^4.17.3"
  },
  "devDependencies": {
    "eslint": "^6.7.0",
    "nodemon": "^2.0.1"
  }
}
